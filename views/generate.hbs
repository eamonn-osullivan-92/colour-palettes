
<div class="generate-container">
    <div class="generate-background">
            {{#each colors}}
                <div class="generate-color" style="background-color:{{this}}">{{this}}</div>
            {{/each}}
        </div>  
        <div class="generate-button-container">
            <a href="/generate">
            <button class="generate-btn btn">
            Generate</button>
            </a>
            <form method="POST" action="/generate">
              <input type="hidden" name="colors" value={{colors}}>
                <div></div>
                <button class="save btn">Save</button>
            </form>


    </div>
</div>


